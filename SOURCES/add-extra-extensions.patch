+        this._hotKeysEnabled = true;
+    }
+
+    _disableHotKeys() {
+        if (!this._hotKeysEnabled)
+            return;
+
+        let keys = ['app-hotkey-', 'app-shift-hotkey-', 'app-ctrl-hotkey-'];
+        keys.forEach( function(key) {
+            for (let i = 0; i < DashToDock_KeyboardShortcuts_NUM_HOTKEYS; i++)
