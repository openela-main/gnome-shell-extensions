+    // Reset the displayed apps icon to mantain the correct order
+    resetAppIcons : function(geometryChange) {
+        let children = this._getTaskbarIcons(true);
+
+        for (let i = 0; i < children.length; i++) {
+            let item = children[i];
+            item.destroy();
+        }
