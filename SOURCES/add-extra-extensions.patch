+    Name: 'DashToPanel.ProgressManager',
+
+    _init: function() {
+
+        this._entriesByDBusName = {};
+
+        this._launcher_entry_dbus_signal_id =
+            Gio.DBus.session.signal_subscribe(null, // sender
