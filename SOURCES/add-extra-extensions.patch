+function getAppStableSequence(app, monitor) {
+    let windows = AppIcons.getInterestingWindows(app, monitor);
+    
+    return windows.reduce((prevWindow, window) => {
+        return Math.min(prevWindow, getWindowStableSequence(window));
+    }, Infinity);
