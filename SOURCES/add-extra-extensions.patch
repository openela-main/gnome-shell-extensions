+                            if (!error.matches(Gio.IOErrorEnum, Gio.IOErrorEnum.CANCELLED)) {
+                                global.log('Error while loading thumbnail: ' + error);
+                                this._icon.child = this._createEmblemedStIcon(this._fileInfo.get_icon(), null);
+                            }
+                        }
+                    }
+                );
+            }
+        }
