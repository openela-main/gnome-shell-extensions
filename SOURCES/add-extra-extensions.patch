+            this._raisedClone.source.opacity = 255;
+            this._raisedClone.destroy();
+        }
+
+        this.callParent('animateOutAndDestroy');
+    },
+
+    // For ItemShowLabel
+    _getIconAnimationOffset: function() {
+        if (!Me.settings.get_boolean('animate-appicon-hover'))
+            return 0;
