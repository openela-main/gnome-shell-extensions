+            this._ignoreAddedChild = [this.panel._centerBox, this.panel._rightBox].indexOf(parent) >= 0;
+
+            parent.remove_child(actor);
+            parent.insert_child_at_index(actor, actorIndex);
+        }
+    }
+    
+});
