+                Utils.animateWindowOpacity(w.get_compositor_private(), tweenOpts);
+            }
+        });
+    },
+
+    _onShowDesktopButtonPress: function() {
+        let label = 'trackerFocusApp';
+
+        this._signalsHandler.removeWithLabel(label);
