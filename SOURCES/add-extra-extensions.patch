+        let sourceAllocation = Utils.getTransformedAllocation(this.currentAppIcon.actor);
+        let [previewsWidth, previewsHeight] = this._getPreviewsSize();
+        let appIconMargin = Me.settings.get_int('appicon-margin') / scaleFactor;
+        let x = 0, y = 0;
