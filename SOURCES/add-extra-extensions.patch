+        this._getAppIcons().forEach(function(icon) {
+            if (!seenApps[icon.app]) {
+                seenApps[icon.app] = 1;
+                counter++;
+            }
+
+            if (counter <= 10) {
+                icon.setNumberOverlay(counter == 10 ? 0 : counter);
+            } else {
+                // No overlay after 10
+                icon.setNumberOverlay(-1);
+            }
+
+            icon.updateHotkeyNumberOverlay();
+        });
