+        let windows = this.getInterestingWindows();
+        let current_workspace = global.workspace_manager.get_active_workspace();
+        for (let i = 0; i < windows.length; i++) {
+            let w = windows[i];
+            if (w.get_workspace() == current_workspace && w.showing_on_its_workspace()) {
