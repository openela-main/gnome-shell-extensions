+                this.menu.add_child(fadeWidgets[0]);
+                this.menu.add_child(fadeWidgets[1]);
+            }
+            
+            fadeWidgets[0].visible = value > 0;
+            fadeWidgets[1].visible = value + pageSize < upper;
