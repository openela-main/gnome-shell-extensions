+            log(`Error: Cant open ${this.file.get_uri()} because it is a broken symlink.`);
+            return;
+        }
+
+        if (this.trustedDesktopFile) {
+            this._desktopFile.launch_uris_as_manager([], context, GLib.SpawnFlags.SEARCH_PATH, null, null);
+            return;
+        }
