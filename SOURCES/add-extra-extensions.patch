+        direction == Meta.MotionDirection.UP_LEFT ||
+        direction == Meta.MotionDirection.UP_RIGHT) {
+        yDest -= Main.panel.height;
+    } else if (direction != Meta.MotionDirection.LEFT &&
+               direction != Meta.MotionDirection.RIGHT) {
+        yDest += Main.panel.height;
+    }
+
+    return [xDest, yDest];
